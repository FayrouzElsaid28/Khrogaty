<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="roqay.task.khrogaty">

    <application
        android:allowBackup="true"
        android:icon="@drawable/khrogaty_logo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/khrogaty_logo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".onboarding.OnBoardingActivity"></activity>
        <activity android:name=".features.HomeActivity" />
        <activity android:name=".SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>